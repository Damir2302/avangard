                                .b-slider-two__item
                                    .b-slider-two__item-title Реализация
                                    img.b-slider-two__item-img(src='assets/images/slider-two_2.jpg' alt='Реализация проекта')

                            .b-slider-two__slide.swiper-slide
                                .b-slider-two__item
                                    .b-slider-two__item-title Проект
                                    img.b-slider-two__item-img(src='assets/images/slider-two_1.jpg' alt='Разработка дизайна')

                            .b-slider-two__slide.swiper-slide
                                .b-slider-two__item
                                    .b-slider-two__item-title Реализация
